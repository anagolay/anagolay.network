{
	"name": "@anagolay/website",
	"version": "0.3.0",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite --port 7776 dev",
		"build": "NODE_ENV=production svelte-kit sync && vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --port 7777",
		"bp": "pnpm build && pnpm preview",
		"prepare": "svelte-kit sync && husky install",
		"test": "playwright test",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pnpm lint:eslint && pnpm lint:prettier",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:prettier": "prettier --ignore-path .gitignore --check .",
		"pretty-quick": "pretty-quick --staged",
		"format": "prettier --ignore-path .gitignore --write ."
	},
	"devDependencies": {
		"@playwright/test": "1.29.0-alpha-nov-30-2022",
		"@sveltejs/adapter-auto": "1.0.0-next.90",
		"@sveltejs/adapter-static": "1.0.0-next.48",
		"@sveltejs/kit": "1.0.0-next.571",
		"@typescript-eslint/eslint-plugin": "5.22.1-alpha.10",
		"@typescript-eslint/parser": "5.22.1-alpha.10",
		"autoprefixer": "10.4.7",
		"eslint": "8.15.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"husky": "7.0.4",
		"marked": "4.0.15",
		"postcss": "8.4.13",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"pretty-quick": "3.1.3",
		"serve": "^13.0.2",
		"svelte": "3.53.1",
		"svelte-check": "2.10.0",
		"svelte-preprocess": "4.10.7",
		"svelte-seo": "1.4.1",
		"sveltejs-adapter-ipfs": "0.3.1",
		"tailwindcss": "3.0.24",
		"tslib": "2.4.0",
		"typescript": "4.6.4",
		"vite": "3.2.4"
	},
	"dependencies": {
		"lottie-web": "^5.9.6"
	}
}
